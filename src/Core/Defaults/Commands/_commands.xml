<?xml version='1.0' encoding='UTF-8' ?>

<services>

    <service id="system.command.debug" class="SpawnCore\Defaults\Services\Commands\DebugCommand">
        <argument type="service" value="system.repository.modules"/>
        <tag>console.command</tag>
    </service>

    <service id="system.command.cache_clear" class="SpawnCore\Defaults\Services\Commands\CacheClearCommand">
        <tag>console.command</tag>
    </service>

    <service id="system.command.spawn_build" class="SpawnCore\Defaults\Services\Commands\SpawnBuildCommand">
        <argument type="service" value="system.database.helper"/>
        <argument type="service" value="system.repository.modules"/>
        <tag>console.command</tag>
    </service>

    <service id="system.command.npm_install_dependencies" class="SpawnCore\Defaults\Services\Commands\NpmInstallCommand">
        <tag>console.command</tag>
    </service>

    <service id="system.command.list_modules" class="SpawnCore\Defaults\Services\Commands\ListModulesCommand">
        <tag>console.command</tag>
    </service>

    <service id="system.command.migration_create" class="SpawnCore\Defaults\Services\Commands\MigrationCreateCommand">
        <tag>console.command</tag>
    </service>

    <service id="system.command.module_create" class="SpawnCore\Defaults\Services\Commands\ModuleCreateCommand">
        <tag>console.command</tag>
    </service>

    <service id="system.command.database_setup_minimal" class="SpawnCore\Defaults\Services\Commands\DatabaseSetupMinimalCommand">
        <tag>console.command</tag>
    </service>

    <service id="system.command.database_update" class="SpawnCore\Defaults\Services\Commands\DatabaseUpdateCommand">
        <tag>console.command</tag>
    </service>

    <service id="system.command.theme_compile" class="SpawnCore\Defaults\Services\Commands\ThemeCompileCommand">
        <tag>console.command</tag>
    </service>

    <service id="system.command.module_refresh" class="SpawnCore\Defaults\Services\Commands\ModulesRefreshCommand">
        <argument type="service" value="system.database.helper"/>
        <argument type="service" value="system.repository.modules"/>
        <argument type="service" value="system.service.seo_url_manager"/>
        <argument type="service" value="system.service.configuration_system"/>
        <argument type="service" value="system.service.snippet_system"/>
        <tag>console.command</tag>
    </service>

    <service id="system.command.migration_execute" class="SpawnCore\Defaults\Services\Commands\MigrationExecuteCommand">
        <argument type="service" value="system.repository.migrations"/>
        <argument type="service" value="system.database.helper"/>
        <tag>console.command</tag>
    </service>

    <service id="system.command.spawn_setup" class="SpawnCore\Defaults\Services\Commands\SpawnSetupCommand">
        <tag>console.command</tag>
    </service>

    <service id="system.command.cron_queue_start" class="SpawnCore\Defaults\Services\Commands\CronQueueCommand">
        <tag>console.command</tag>
    </service>


</services>
