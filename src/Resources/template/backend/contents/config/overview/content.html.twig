{% block content_inner %}

    {% include 'backend/contents/misc/headline.html.twig' with {
        title: 'System configuration'
    } only %}


    <div class="container">

        <ul class="button-list ">
            {% for folder in available_folders %}
                <li data-backend-content-link="{{ seo_url('system.backend.system_config', 'folder', [folder]) }}">
                    <div class="button-list-element">
                        {{ folder }}
                    </div>
                </li>
            {% endfor %}
        </ul>

    </div>


{% endblock %}