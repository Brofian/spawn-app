<?xml version='1.0' encoding='UTF-8' ?>

<services>

    <import file="/Commands/_commands.xml"/>


    <service id="system.service.configuration_manager" class="spawnApp\Services\ConfigurationManager">
        <argument type='service' value="system.repository.configuration"/>
        <tag>service</tag>
    </service>


    <service id="system.service.seo_url_manager" class="spawnApp\Services\SeoUrlManager">
        <argument type='service' value="system.repository.seo_urls"/>
        <tag>service</tag>
    </service>


    <service id="system.service.admin_login_manager" class="spawnApp\Services\AdminLoginManager" static="true">
        <argument type='service' value="system.repository.administrator"/>
        <argument type='service' value="system.session.helper"/>
        <tag>service</tag>
        <tag>event.subscriber</tag>
    </service>


</services>

