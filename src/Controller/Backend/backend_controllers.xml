<?xml version='1.0' encoding='UTF-8' ?>

<services>

    <service id="system.backend.base" class="spawnApp\Controller\Backend\BaseBackendController">
        <tag>backend.controller</tag>
    </service>

    <service id="system.backend.seo_url_config" class="spawnApp\Controller\Backend\SeoUrlConfigController">
        <argument type='service' value="system.kernel.request"/>
        <argument type='service' value="system.service.seo_url_manager"/>
        <tag>backend.controller</tag>
    </service>

    <service id="system.backend.administrator" class="spawnApp\Controller\Backend\AdminLoginController">
        <argument type='service' value="system.service.admin_login_manager"/>
        <argument type='service' value="system.csrf_token.helper"/>
        <tag>base.controller</tag>
    </service>

    <service id="system.backend.system_config" class="spawnApp\Controller\Backend\SystemConfigController">
        <argument type='service' value="system.repository.configuration"/>
        <tag>backend.controller</tag>
    </service>

</services>

