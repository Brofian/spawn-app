<?xml version='1.0' encoding='UTF-8' ?>

<services>

    <service
            id="spawn.app.twig.extension.filter.seo_url_rewrite"
            class="spawnApp\Extensions\Twig\SeoUrlRewriteFilter"
            static="true"
    >
        <tag>extension.twig</tag>
    </service>


    <service
            id="spawn.app.twig.extension.function.csrf_token"
            class="spawnApp\Extensions\Twig\CSRFTokenFunction"
            static="true"
    >
        <tag>extension.twig</tag>
    </service>

    <service
            id="spawn.app.twig.extension.function.form_honeypot"
            class="spawnApp\Extensions\Twig\FormHoneypotFunction"
            static="true"
    >
        <tag>extension.twig</tag>
    </service>


</services>
